<function>
	<name dialects="oxn&ge;1.3" modules="oxne&ge;1.3">trReplace</name>
	<syntax>
		<syn>
			<opt>the</opt> <me/> of ( <mv>haystack</mv>, <mv>needle</mv>, <mv>noodle</mv> )
		</syn>
		<syn>
