<function>
	<name dialects="xn,oxn" modules="oxns">aug</name>
	<syntax>
		<syn>
			<opt>the</opt> <me/> of <mv>factor</mv>
		</syn>
		<syn>
			<me/> ( <mv>expression</mv> )
		</syn>
		<p>
			<mv>Factor</mv> and <mv>expression</mv> yield <a>dt:integer</a>s,
			<a>dt:number</a>s, or <a>dt:complex</a>es.
		</p>
	</syntax>
	<examples>
